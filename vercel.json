{
  "version": 2,
  "functions": {
    "api/predict.py": {
      "runtime": "python3.11"
    },
    "api/metrics.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { "src": "/api/predict", "dest": "api/predict.py" },
    { "src": "/api/metrics", "dest": "api/metrics.py" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(index|transaction).html", "dest": "/$1.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
